{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "iComponents/**/*.tsx","app/page.jsx", "components/inventory/AddProduct.jsx", "app/dashboard/inventory/products/page.jsx", "components/inventory/Modal.jsx", "app/dashboard/inventory/tags/page.jsx", "app/dashboard/inventory/tags/add/page.jsx", "components/manager/AddManager.jsx", "components/Doctors/AddDoctor.jsx", "app/dashboard/manager/page.jsx", "app/dashboard/doctor/page.jsx"],
  "exclude": ["node_modules"]
}
